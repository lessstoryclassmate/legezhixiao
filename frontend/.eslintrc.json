{
  "root": true,
  "env": {
    "node": true,
    "browser": true,
    "es2022": true
  },
  "extends": [
    "eslint:recommended"
  ],
  "rules": {
    "no-console": "warn",
    "no-debugger": "warn",
    "no-unused-vars": "warn"
  },
  "ignorePatterns": [
    "dist",
    "node_modules",
    "*.config.js"
  ]
}
